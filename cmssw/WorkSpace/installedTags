#instructions for v45_0 of cfA

export SCRAM_ARCH="slc5_amd64_gcc434"
cmsrel CMSSW_4_2_3
cd CMSSW_4_2_3/src
cmsenv
addpkg PhysicsTools/PatAlgos V08-06-24

# deterministic calculation of FastJet corrections
addpkg RecoJets/Configuration V02-04-16
addpkg RecoJets/JetAlgorithms V04-01-00
addpkg RecoJets/JetProducers V05-05-03

addpkg MuonAnalysis/MuonAssociators V01-13-00
addpkg PhysicsTools/Configuration V00-10-10

cvs co -r cfA_45 -d Workspace/ConfigurableAnalysis UserCode/JRVlimant/cmssw/WorkSpace/ConfigurableAnalysis
cvs co -r UCSBv33_0 -d Workspace/Dictionaries UserCode/JRVlimant/cmssw/WorkSpace/Dictionaries
cvs co -r UCSBv33_0 -d Workspace/BFieldProducer UserCode/JRVlimant/cmssw/WorkSpace/BFieldProducer
cvs co -r cfA_45 -d Workspace/JetCorrProducer UserCode/JRVlimant/cmssw/WorkSpace/JetCorrProducer

scram b -c

